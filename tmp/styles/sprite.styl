$svg-common 
	background url("sprite.svg") no-repeat

$custommenu--closed
	@extend $svg-common
	background-position 0 0
	width 16px
	height 16px

$custommenu--open
	@extend $svg-common
	background-position 0 9.090909090909092%
	width 16px
	height 16px

$menu--closed
	@extend $svg-common
	background-position 0 18.181818181818183%
	width 16px
	height 16px

$menu--open
	@extend $svg-common
	background-position 0 27.272727272727273%
	width 16px
	height 16px

$myelem--dog
	@extend $svg-common
	background-position 0 50%
	width 64px
	height 64px

$page--cat
	@extend $svg-common
	background-position 0 100%
	width 64px
	height 64px

